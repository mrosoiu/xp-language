package net.xp_forge.examples.enum.coin {

  /**
   * Prints all coins' values
   *
   * @purpose  Demo
   */
  class Values extends util.cmd.Command {
  
    /**
     * Run this command
     *
     */
    public function run() {
      foreach (Coin::values() as $coin) {
        $this->out->writeLine($coin->name, ': ', $coin->value(), '¢ (', $coin->color(), ')');
      }
    }
  }
}
